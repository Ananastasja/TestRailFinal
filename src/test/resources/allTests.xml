<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="All tests" parallel="classes">
    <listeners>
        <listener class-name="util.TestListener"/>
        <listener class-name="util.AnnotationTransformer"/>
        <listener class-name="util.DataProviderTransformer"/>
    </listeners>
    <test name="All tests">
        <classes>
            <class name="ui_tests.LoginTest"></class>
            <class name="ui_tests.TestCaseTest"></class>
            <class name="ui_tests.MilestoneTest"></class>
            <class name="api_tests.ProjectTest"></class>
            <class name="api_tests.SuiteTest"></class>
        </classes>
    </test>
</suite>
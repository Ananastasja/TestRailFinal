<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG with Retry">
    <listeners>
        <listener class-name="util.TestListener"/>
        <listener class-name="util.AnnotationTransformer"/>
    </listeners>
    <test name="Tests" preserve-order="true">
        <classes>
            <class name="ui_tests.LoginTest"/>
            <class name="ui_tests.MilestoneTest"/>
            <class name="ui_tests.TestCaseTest"/>
            <class name="api_tests.ProjectTest"/>
            <class name="api_tests.SuiteTest"/>
        </classes>
    </test>
</suite>